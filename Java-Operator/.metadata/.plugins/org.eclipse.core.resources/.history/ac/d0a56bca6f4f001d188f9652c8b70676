
public class LogicalOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         int a=20;
         int b=10;
         int c=30;
         if(a>b && a>c)
         {
          System.out.println("A is Greater"+a);
         }
         else if(b>c)
         {
        	 System.out.println("B is Greater"+b);
         }
         else {
        	 System.out.println("C is Greater="+c);
         }
         
	}

}
